{
  "manifest_version": 3,
  "name": "Auto Adfocus Ads",
  "version": "1.0",
  "description": "Updates URLs to ads, And has ad banner on all websites.",
  "permissions": ["declarativeContent"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
